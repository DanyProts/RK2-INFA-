#include <iostream>
#include <bitset>
using namespace std;
int main() {
    string binaryA = "101101";
    string binaryB = "110011";
    int A = stoi(binaryA, nullptr, 2); 
    int B = stoi(binaryB, nullptr, 2);
    int andResult = A & B;
    cout << "Результат AND: " << bitset<6>(andResult) << endl;
    int sum = A + B;
    cout << "Сумма чисел A и B : " << bitset<6>(sum) << endl;
    return 0;
}
